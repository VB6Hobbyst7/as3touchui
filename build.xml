<?xml version="1.0" encoding="utf-8"?>
<project name="as3touchui" default="compile" basedir=".">
	<property name="statics.svn" value="${basedir}/.."/>
	<property file="${statics.svn}/other/ant/build.properties"/>
	<import file="${statics.svn}/other/ant/build.xml"/>
	<property name="statics.savePath" value="${statics.svn}/${libPath}/statics.swc"/>

	<target name="compile">
		<echo message="${statics.svn}/${FLEX_HOME}"></echo>
		<antcall target="compile swc">
			<param name="isDev" value="false"/>
			<param name="isRelease" value="true"/>
			<param name="src" value="${basedir}"/>
			<param name="filePath" value="${statics.savePath}"/>
			<param name="FLEX_HOME" value="${statics.svn}/${FLEX_HOME}"/>
			<param name="libPath" value="${statics.svn}/${libPath}"/>
			<param name="libSwc" value=""/>
		</antcall>
	</target>
</project>
